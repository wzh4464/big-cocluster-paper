@article{wu2023EffectiveClusteringStructured,
  title      = {Effective Clustering via Structured Graph Learning},
  author     = {Wu, Danyang and Nie, Feiping and Lu, Jitao and Wang, Rong and Li, Xuelong},
  year       = {2023},
