  keywords   = {/unread,Adaptation models,Adaptive graph convolution,attributed graph clustering,Automatic generation control,Clustering methods,Convolution,Feature extraction,graph clustering,low-pass graph filter,Proteins,Social networking (online)},
  annotation = {影响因子: 8.9\\
